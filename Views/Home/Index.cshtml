@{
  ViewData["Title"] = "Home Page";
}

<div class="container mt-5">
  <div class="row justify-content-center">
    <div class="col-md-8 text-center">
      <h2>Welcome to our Book Management System</h2>
      <p class="lead mt-4">Explore our collection of books categorized by authors</p>
    </div>
  </div>
  <div class="row mt-5">
    <div class="col-md-4 mb-4">
      <div class="card h-100">
        <img
          src="data:image/jpeg;base64,"
          class="card-img-top" alt="..." style="height: 200px;object-fit: cover;">
        <div class="card-body">
          <h5 class="card-title">William Shakespeare</h5>
          <p class="card-text">Explore books by William Shakespeare</p>
          <a href="@Url.Action("Index", "Livre")" class="btn btn-primary">View Books</a>
        </div>
      </div>
    </div>
    <div class="col-md-4 mb-4">
      <div class="card h-100">
        <img
          src="data:image/jpeg;base64,"
          class="card-img-top" alt="..." style="height: 200px;object-fit: cover;">
        <div class="card-body">
          <h5 class="card-title">Victor Hugo</h5>
          <p class="card-text">Explore books by Victor Hugo</p>
          <a href="@Url.Action("Index", "Livre")" class="btn btn-primary">View Books</a>
        </div>
      </div>
    </div>
    <div class="col-md-4 mb-4">
      <div class="card h-100">
        <img
          src="data:image/jpeg;base64,"
          class="card-img-top" alt="..." style="height: 200px;object-fit: cover;">
        <div class="card-body">
          <h5 class="card-title">Leo Tolstoy</h5>
          <p class="card-text">Explore books by Leo Tolstoy</p>
          <a href="@Url.Action("Index", "Livre")" class="btn btn-primary">View Books</a>
        </div>
      </div>
    </div>
  </div>
</div>

@section Styles {
  <style>
    .card {
      transition: all 0.3s;
      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    }

    .card:hover {
      box-shadow: 0 8px 12px rgba(0, 0, 0, 0.2);
    }
  </style>
}
